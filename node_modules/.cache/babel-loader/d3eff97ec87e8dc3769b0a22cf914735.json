await axios({\r\n        method: \"post\",\r\n        url: blackBoxUrl + \"Transactions/TransactionType_Fetch\",\r\n        headers: {\r\n          \"Content-Type\": \"multipart/form-data\",\r\n        },\r\n      });\r\n\r\n      this.transactionTypes = response.data.data;\r\n    },\r\n    async showTransactionTypeDialog(data = \"\", action) {\r\n      this.showDialog = true;\r\n      this.transactionType = data == \"\" ? this.transactionType : data;\r\n      this.dialogAction = action;\r\n    },\r\n    async showResponse(e) {\r\n      this.resetTransactionType();\r\n      if (e === \"\") {\r\n        this.list();\r\n        this.showDialog = false;\r\n        this.dialogAction = \"\";\r\n      } else {\r\n        if (e.data.color === \"error\") {\r\n          await this.resetSnackBar();\r\n          this.snackBar.message = e.data.message;\r\n          this.snackBar.color = \"error\";\r\n          this.snackBar.display = true;\r\n        } else {\r\n          this.list();\r\n          this.showDialog = false;\r\n          this.dialogAction = \"\";\r\n\r\n          if (e.data.message.length !== 0) {\r\n            await this.resetSnackBar();\r\n            this.snackBar.message = e.data.message;\r\n            this.snackBar.color = \"success\";\r\n            this.snackBar.display = true;\r\n          }\r\n        }\r\n      }\r\n    },\r\n    getUserAccess() {\r\n      let fd = new FormData();\r\n      fd.set(\"function\", \"GetUserAccess\");\r\n      fd.set(\"usertype\", this.$session.get(\"UserType\"));\r\n\r\n      let init = {\r\n        method: \"POST\",\r\n        body: fd,\r\n        mode: \"cors\",\r\n      };\r\n      fetch(\"http://34.96.254.26/LESS/UAC/lessactions.php\", init).then(\r\n        async (res) => {\r\n          let result = await res.json();\r\n          result.map((x) => {\r\n            this.useraccess = x.managetransactiontype[0];\r\n          });\r\n        }\r\n      );\r\n    },\r\n  },\r\n  mounted() {\r\n    this.list();\r\n    this.getUserAccess();\r\n  },\r\n};\r\n</script>\r\n<style lang=\"postcss\" scoped>\r\n.container-div {\r\n  margin: 1%;\r\n  width: 100% !important;\r\n}\r\n.pt-2 {\r\n  padding-top: 2% !important;\r\n}\r\n.pb-2 {\r\n  padding-bottom: 2% !important;\r\n}\r\n</style>\r\n"]}]}