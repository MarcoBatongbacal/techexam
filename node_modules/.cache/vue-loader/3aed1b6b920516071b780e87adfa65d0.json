flat-title\">\r\n        <h3>Manage Transaction Type</h3>\r\n      </v-card-title>\r\n      <div class=\"table-container ml-5 mr-5\">\r\n        <v-row dense>\r\n          <v-col cols=\"8\">\r\n            <ul class=\"inline-action\">\r\n              <li>\r\n                <v-btn class=\"mr-2\" small @click=\"list()\">\r\n                  <v-icon dark>\r\n                    mdi-refresh\r\n                  </v-icon>\r\n                  Refresh list\r\n                </v-btn>\r\n              </li>\r\n              <li>\r\n                <v-btn\r\n                  class=\"mr-2\"\r\n                  @click=\"showTransactionTypeDialog('', 'Add')\"\r\n                  :disabled=\"\r\n                    useraccess.create_managetransactiontype ? false : true\r\n                  \"\r\n                >\r\n                  <v-icon left dark>\r\n                    mdi-plus\r\n                  </v-icon>\r\n                  Add\r\n                </v-btn>\r\n              </li>\r\n            </ul>\r\n          </v-col>\r\n          <v-col cols=\"4\">\r\n            <v-text-field\r\n              v-model=\"search\"\r\n              append-icon=\"mdi-magnify\"\r\n              label=\"Search\"\r\n              single-line\r\n              hide-details\r\n              class=\"mb-3 pull-right\"\r\n            ></v-text-field>\r\n          </v-col>\r\n          <v-col cols=\"12\">\r\n            <v-data-table\r\n              :headers=\"headers\"\r\n              :items=\"transactionTypes\"\r\n              :search=\"search\"\r\n              class=\"flat-table\"\r\n            >\r\n              <template v-slot:item=\"row\">\r\n                <tr>\r\n                  <td>{{ row.item.transaction_type_name }}</td>\r\n                  <td>{{ row.item.transaction_type_code }}</td>\r\n                  <td>\r\n                    <v-btn\r\n                      class=\"mx-2\"\r\n                      icon\r\n                      small\r\n                      col